{
  "name": "und-css",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "serve": "browser-sync dist -w --no-notify",
    "build": "cross-env NODE_ENV=production ELEVENTY_ENV=production run-s clean build:*",
    "build:styles": "node-sass src/styles.scss dist/css/styles.css",
    "build:styles:minified": "node-sass src/styles.scss dist/css/styles.min.css --output-style compressed",
    "build:eleventy": "eleventy --quiet",
    "lint": "stylelint \"./src/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "dev": "cross-env NODE_ENV=development ELEVENTY_ENV=development npm-run-all clean build:styles --parallel start:* serve",
    "start:styles": "node-sass -w src/ -o dist/css/",
    "start:eleventy": "eleventy --quiet --watch"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "rfs": "^9.0.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.12",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0"
  }
}
